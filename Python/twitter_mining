import tweepy
from tweepy import OAuthHandler

consumer_key = '0iYVL3ivhUtkfUDoKDAOSuEpm'
consumer_secret = 'CWbRQ3kEXlIbzBrpPOo8vHPnxfECcZrUyAPSIjoclDejY8VI75'
access_token = '1460157939210240003-z5X6hAj1ioYOSCIRcBOsGc5J3vh7nw'
